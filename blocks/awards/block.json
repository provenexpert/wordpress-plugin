{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "provenexpert/awards",
	"version": "1.0.0",
	"title": "ProvenExpert Awards",
	"category": "widgets",
	"description": "Provides a block to show your ProvenExpert Awards.",
	"keywords": ["ProvenExpert"],
	"supports": {},
	"textdomain": "provenexpert",
	"editorScript": "file:build/index.js",
	"editorStyle": "file:build/index.css",
    "style": "file:build/style-index.css",
	"attributes": {
        "preview": {
            "type": "boolean",
            "default": false
        },
		"blockId": {
			"type": "string"
		},
        "width": {
            "type": "integer",
            "default": 100
        },
        "fixed": {
            "type": "boolean",
            "default": false
        },
        "origin": {
            "type": "string",
            "default": "bottom"
        },
        "position": {
            "type": "integer",
            "default": 0
        },
        "award_type": {
            "type": "string",
            "default": "recommend"
        }
    },
	"example": {
		"attributes": {
            "preview": true
        }
	}
}
